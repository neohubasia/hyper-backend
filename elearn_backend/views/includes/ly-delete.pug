div.modal.fade#dialogDeleteConfirm(tabindex='-1', role='dialog', aria-labelledby='deleteConfirm')
  div.modal-dialog.modal-sm(role='document')
    div.modal-content
      div.modal-header
        h4.modal-title Delete
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
      div.modal-body
        p Are you sure to delete this?
      div.modal-footer.justify-content-md-center
        button.btn.btn-secondary.btn-sm#dialogCancel(type='button', data-dismiss='modal')
          | CANCEL
        button.btn.btn-danger.btn-sm#dialogAccept(type='button', data-dismiss='modal')
          i.fa.fa-check   OK

div.col-sm-12
  div.col-sm-12.alert.alert-success.alert-dismissible#alertDeleteSuccess(role='alert' style="display:none;")
    strong Success:
    span#delSuccessMsg  Delete Successful.
    button.close(type='button', data-hide='alert', aria-label='Close')
      span(aria-hidden='true') &times;

div.col-sm-12
  div.col-sm-12.alert.alert-danger.alert-dismissible#alertDeleteError(role='alert' style="display:none;")
    strong Error:
    span#delErrorMsg  Delete Unsuccessful.
    button.close(type='button', data-hide='alert', aria-label='Close')
      span(aria-hidden='true') &times;